{
  "expo": {
    "name": "Webmenu",
    "slug": "Webmenu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/custom.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/custom.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.editor16.Webmenu"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
        {
          "fonts": ["./assets/fonts/Pacifico-Regular.ttf"]
        }
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a509fbfa-24d1-49cd-91d2-5d9ad6497d5b"
      }
    }
  }
}
